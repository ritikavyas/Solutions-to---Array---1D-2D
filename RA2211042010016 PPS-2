#include<stdio.h>
#include<conio.h>
int main()
{
    int arr[50], size, i;
    printf("How many Elements You want to store in Array ? ");
    scanf("%d", &size);
    printf("\nEnter %d Elements: ", size);
    for(i=0; i<size; i++)
        scanf("%d", &arr[i]);
    printf("\nElements of this One-dimensional Array is:\n");
    for(i=0; i<size; i++)
        printf("%d ", arr[i]);
    getch();
    return 0;
}
